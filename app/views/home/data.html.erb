<style>
  .container {
    max-width: 600px;
    margin: 50px auto;
    padding: 20px;
    border-radius: 8px;
    box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);
    background-color: #f9f9f9;
  }

  h1 {
    text-align: center;
    margin-bottom: 30px;
    color: #333;
  }

  .details {
    display: flex;
    flex-direction: column;
    gap: 20px;
  }

  .detail-item {
    display: flex;
    justify-content: space-between;
    align-items: center;
    border-bottom: 1px solid #ddd;
    padding-bottom: 10px;
    font-size: 18px;
  }

  .detail-item strong {
    color: #555;
  }

  .image-container {
    text-align: center;
  }

  .image-container img {
    max-width: 150px;
    border-radius: 8px;
    box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);
  }
  .pass{
    text-align: center;
    font-size: 20px;
    font-weight: bold;
    color: #00FF00; /* Initial text color */
  }
.fail{
        text-align: center;
        font-size: 20px;
        font-weight: bold;
        color: #FF0000; /* Initial text color */
}
</style>
<div>
    <div class="container">
        <h1>Extracted Details from ID</h1>

        <div class="details-container">

        </div>
    </div>
    <p class="pass">Judgement : Good<p>
    <p class="fail"> Judgement : Bad<p>

    <script>
        fetch('/output.json') 
        .then(response => response.json())
        .then(data => {
          const detailsContainer = document.querySelector('.details-container');

          for (const [key, value] of Object.entries(data)) {
            const detailItem = document.createElement('div');
            detailItem.classList.add('detail-item');

            const keyElement = document.createElement('strong');
            keyElement.textContent = key;

            const valueElement = document.createElement('span');
            valueElement.textContent = value;

            detailItem.appendChild(keyElement);
            detailItem.appendChild(valueElement);

            detailsContainer.appendChild(detailItem);
          }
        });
    </script>
</div>